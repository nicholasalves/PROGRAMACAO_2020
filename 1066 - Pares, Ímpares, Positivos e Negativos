#include <stdio.h>
 
int main(void) {
    int par=0, impar=0, positivo=0, negativo=0;
    int x,y;
        for(y=0;y<5;y++){
            scanf("%d",&x);
                if(x%2==0)
                par++;
            else if((x%2!=0))
                impar++;
            if((x>0))
                positivo++;
             if((x<0))
                negativo++;
        }
            printf("%d valor(es) par(es)\n", par);
            printf("%d valor(es) impar(es)\n", impar);
            printf("%d valor(es) positivo(s)\n", positivo);
            printf("%d valor(es) negativo(s)\n", negativo);
            
            return 0;
        }
